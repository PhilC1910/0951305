<?xml version="1.0" encoding="utf-8"?>
<com.cours5b5.philippechevry.vues.VParametres xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activites.AActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/hauteur"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="70dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="52dp"
        android:text="@string/hauteur"
        app:layout_constraintBottom_toTopOf="@+id/largeur"
        app:layout_constraintEnd_toStartOf="@+id/hauteurNombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hauteurNombre" />

    <TextView
        android:id="@+id/largeur"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="244dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="5dp"
        android:text="@string/largeur"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/largeurNombre"
        app:layout_constraintStart_toStartOf="@+id/pourGagner"
        app:layout_constraintTop_toBottomOf="@+id/hauteur" />

    <TextView
        android:id="@+id/pourGagner"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="133dp"
        android:layout_marginEnd="53dp"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:text="@string/pourGagner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pourGagnerNombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/largeur" />

    <Spinner
        android:id="@+id/hauteurNombre"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_marginBottom="68dp"
        android:layout_marginEnd="91dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="0dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/largeurNombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hauteur"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <Spinner
        android:id="@+id/largeurNombre"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="136dp"
        android:layout_marginEnd="83dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="5dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/largeur"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.692" />

    <Spinner
        android:id="@+id/pourGagnerNombre"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginBottom="110dp"
        android:layout_marginEnd="91dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pourGagner"
        app:layout_constraintTop_toBottomOf="@+id/largeurNombre" />

</com.cours5b5.philippechevry.vues.VParametres>